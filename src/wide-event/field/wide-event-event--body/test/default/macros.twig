{% macro render(field) %}
    {% set items = [] %}
    
    {% for item in field.items %}
        {% set items = items|merge([{content: _self.renderBody(item)}]) %}
    {% endfor %}
    
    {% include '../../src/index.twig' with {
    attributes: field.attributes
    } %}
{% endmacro %}

{% macro renderBody(item) %}
    {% include "./data/body.html" %}
{% endmacro %}