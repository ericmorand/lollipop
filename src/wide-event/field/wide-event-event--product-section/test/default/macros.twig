{% macro render(field) %}
    {% set items = [] %}
    
    {% import "../../../../paragraph/wide-event-product-section/test/default/macros.twig" as paragraphMacros %}
    
    {% for item in field.items %}
        {% set items = items|merge([{
        content: paragraphMacros.render(item),
        id: item.bundle() ~ '_' ~ item.id(),
        title: 'test'
        }]) %}
    {% endfor %}
    
    {% include "../../src/index.twig" with {
    attributes: field.attributes,
    paragraph: field,
    show_section_links: field.show_section_links,
    sections: field.sections
    } %}
{% endmacro %}