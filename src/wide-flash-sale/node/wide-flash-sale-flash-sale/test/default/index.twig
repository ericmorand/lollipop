<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{{ demo.title }}</title>
    <link rel="stylesheet"
          href="https://www.tagheuer.com/themes/custom/sapient_classy/css/tag_css/sapient.ver.1.0.0.css">
    <link rel="stylesheet" href="../css/index.css">
</head>
<body class="{{ demo.class }}">
<div>
    {% for fixture in fixtures %}
        <div class="{{ demo.class }}--fixture">
            <h1>{{ fixture.title }}</h1>
            <div>
                {% import _self as this %}
                
                {% macro renderVideoSample() %}
                    {% include "./data/templates/field_flash_sale_video.html" %}
                {% endmacro %}
                
                {#{% set content = fixture.content|merge({#}
                {#field_flash_sale_video: this.renderVideoSample()#}
                {#}) %}#}
                
                {% include '../../src/index.twig' with fixture %}
            </div>
        </div>
    {% endfor %}
</div>
<script src="../js/index.js"></script>
</body>
</html>
